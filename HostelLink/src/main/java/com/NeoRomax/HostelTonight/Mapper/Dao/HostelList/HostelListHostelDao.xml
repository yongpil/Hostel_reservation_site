<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.NeoRomax.HostelTonight.HostelList.Dao.HostelListImgDao">
	<select id="getHImgList" resultType="com.NeoRomax.HostelTonight.HostelList.Dto.HostelListImgDto">
		SELECT * FROM  HOSTELS_IMAGE WHERE HSTNUM = #{PARAM1}
	</select>
	
	<insert id="addHImg">
		insert into hostels_image (hstNum,imageName) values( #{param1}, #{param2})
	</insert>
	
</mapper>



















