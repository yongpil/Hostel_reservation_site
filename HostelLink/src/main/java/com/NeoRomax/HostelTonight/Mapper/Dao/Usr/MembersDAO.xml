<mapper namespace="com.NeoRomax.HostelTonight.Usr.Dao.MembersDAO">

	<insert id="joinDao">
		INSERT INTO user_information (ID, PASSWORD, FIRST_NAME, LAST_NAME, BIRTH, enabled) VALUES 
		(#{param1}, #{param2}, #{param3}, #{param4},#{param5}, '1')
	</insert>
	<insert id="joinAuthDao">
		INSERT INTO user_auth (authority) VALUES (#{param1})
	</insert>
</mapper>
